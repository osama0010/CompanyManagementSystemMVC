@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Are You Sure To Delete @Model.Name Record</h1>
<br />

<form asp-action="Delete" method="post">
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input class="form-control" asp-for="Code" readonly/>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" readonly/>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="DateOfCreation"></label>
        <input class="form-control" asp-for="DateOfCreation" readonly/>
    </div>
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>
